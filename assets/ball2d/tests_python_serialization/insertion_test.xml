  <gravity fx="0.0" fy="-10.0"/>

  <static_plane x="0.0 -10.0" n="0.0 1.0"/>
  <static_plane x="-10.0 -5.0" n="1.0 0.0"/>
  <static_plane x=" 10.0 -5.0" n="-1.0 0.0"/>

  <static_plane_renderer plane="0" r="0.1 20.0" color="0.824 0.824 0.824"/>
