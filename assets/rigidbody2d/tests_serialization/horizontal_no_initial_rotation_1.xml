<!--
  A translating ball collides against a horizontl plane and picks up some spin.
-->

<rigidbody2d_scene>

  <camera center="0.0 -0.765207" scale="2.0" fps="50" render_at_fps="0" locked="0"/>

  <sobogus_friction_solver mu="0.3" CoR="1.0" max_iters="50000" eval_every="25" tol="1.0e-12" staggering="geometric" cache_impulses="none"/>

  <integrator type="symplectic_euler" dt="0.01"/>

  <static_plane x="0 -2" n="0 1"/>

  <static_plane_renderer plane="0" r="0.05 5.85" color="0 0 0"/>

  <geometry type="circle" r="0.1"/>

  <rigid_body x="1.9 0.0" theta="0.0" v="-2.0 -2.0" omega="0.0" rho="3.4" geo_idx="0"/>

</rigidbody2d_scene>
