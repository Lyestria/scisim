<rigidbody2d_scene>

  <camera center="0 1" scale="5" fps="50" render_at_fps="1" locked="0"/>

  <integrator type="verlet" dt="0.001"/>

  <sobogus_friction_solver mu="0.0" CoR="0.3" max_iters="50000" eval_every="25" tol="1.0e-12" staggering="geometric" cache_impulses="none"/>

  <near_earth_gravity f="0.0 -10.0"/>

  <static_plane x="0 0" n="0 1"/>

  <static_plane_renderer plane="0" r="0.1 9.0" color="0 0 0"/>

  <geometry type="box" r="0.2 0.2"/>
  <geometry type="box" r="0.4 0.4"/>
  <geometry type="box" r="1 1"/>

  <rigid_body x="-3 2" theta="0" v="0 0" omega="0" rho="1" geo_idx="0"/>
  <rigid_body x="0 2" theta="0" v="0 0" omega="0" rho="1" geo_idx="1"/>
  <rigid_body x="3 2" theta="0" v="0 0" omega="0" rho="1" geo_idx="2"/>

</rigidbody2d_scene>
