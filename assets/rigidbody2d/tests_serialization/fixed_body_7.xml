<rigidbody2d_scene>

  <camera center="0 0" scale="6.5" fps="50" render_at_fps="0" locked="0"/>

  <integrator type="verlet" dt="0.01"/>

  <near_earth_gravity f="0.0 -10.0"/>

  <sobogus_friction_solver mu="0" CoR="1" max_iters="50000" eval_every="25" tol="1.0e-12" staggering="geometric" cache_impulses="none"/>

  <static_plane x="0 0" n="0 1"/>

  <static_plane_renderer plane="0" r="0.1 5.85" color="0 0 0"/>

  <geometry type="circle" r="0.5"/>

  <rigid_body x="0 2" theta="0" v="0 0" omega="3" rho="1" fixed="0" geo_idx="0"/>
  <rigid_body x="-2 1" theta="0" v="0 0" omega="0" rho="1" fixed="1" geo_idx="0"/>
  <rigid_body x="2 1" theta="0" v="0 0" omega="0" rho="1" fixed="1" geo_idx="0"/>
  <rigid_body x="-2 3" theta="0" v="0 0" omega="-5" rho="1" fixed="0" geo_idx="0"/>
  <rigid_body x="2 3" theta="0" v="0 0" omega="5" rho="1" fixed="0" geo_idx="0"/>

</rigidbody2d_scene>
